<div class="col-lg-12">
    <h1>{{this.get_name}}</h1>
    <div class="well bs-component">
        <form id="{{this.get_name}}-form" class="form-horizontal">
            <fieldset>
                <legend>Legend</legend>
                <!-- Form Group Leader provides an insertion point -->
                <div class="form-group-leader"></div>
                <div class="form-group">
                    <div class="col-lg-10 col-lg-offset-2">
                        <button class="btn btn-default">Cancel</button>
                        <button id="{{this.get_name}}-submit" class="btn btn-primary" type="submit">Submit</button>
                    </div>
                </div>
            </fieldset>
        </form>
    <div class="btn btn-primary btn-xs" id="source-button" style="display: none;">&lt; &gt;</div>
    </div>
</div>